

#include <stdio.h>

void main(void)
{
	int mark;
	char grade;

	printf("Please enter your mark.\n");
	scanf_s("%d", &mark);

	if (mark >= 80)
	{
		grade = 'A';
	}
	else if (mark >= 70)
	{
		grade = 'B';

	}
	else if (mark >= 60)
	{
		grade = 'C';
	}
	else if (mark >= 50)
	{
		grade = 'D';
	}
	else
	{
		grade = 'F';
	}

	printf("Your grade is %c\n", grade);
}

